{
  "name": "website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "concurrently \"npm run start:api\" \"vite\"",
    "build": "tsc -b && npm run generate-sitemap && npm run generate-html-sitemap && npm run generate-seo && vite build",
    "postbuild": "npm run notify-google",
    "lint": "eslint .",
    "preview": "concurrently \"npm run start:api\" \"vite preview\"",
    "start:api": "python3 DB/api.py",
    "generate-sitemap": "tsx scripts/generateSitemap.ts",
    "generate-html-sitemap": "tsx scripts/generateHTMLSitemap.ts",
    "generate-seo": "tsx scripts/generateSEO.ts",
    "notify-google": "tsx scripts/notifyGoogleOfSitemap.ts"
  },
  "dependencies": {
    "@stripe/react-stripe-js": "^3.1.1",
    "@stripe/stripe-js": "^5.7.0",
    "@supabase/supabase-js": "^2.48.1",
    "@types/uuid": "^10.0.0",
    "concurrently": "^7.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "flask": "^0.2.10",
    "flask-cors": "^0.0.1",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-rating": "^2.0.5",
    "react-router-dom": "^7.1.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.13.4",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "postcss": "^8.4.49",
    "react-scroll-parallax": "^3.4.5",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.7.1",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "wait-on": "^8.0.2"
  }
